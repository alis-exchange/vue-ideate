{
  "name": "@alis-build/vue-ideate",
  "version": "1.1.1",
  "description": "Vue 3 composables and components to help integrate Alis Ideate into your products.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.ts",
        "require": "./dist/index.d.cts"
      },
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "vue",
    "composables",
    "vue3"
  ],
  "author": "",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alis-exchange/vue-ideate.git"
  },
  "bugs": {
    "url": "https://github.com/alis-exchange/vue-ideate/issues"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vue": "^3.5.25"
  },
  "dependencies": {
    "@vueuse/core": "^14.1.0",
    "buffer": "^6.0.3"
  }
}